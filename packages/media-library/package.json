{
    "name": "@sonantica/media-library",
    "version": "0.1.0",
    "description": "Media library indexing and metadata management for Sonántica",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "clean": "rimraf dist",
        "test": "echo \"No tests yet\"",
        "lint": "echo \"No linting configured yet\""
    },
    "keywords": [
        "sonantica",
        "media",
        "library",
        "metadata"
    ],
    "author": "Sonántica Team",
    "license": "Apache-2.0",
    "dependencies": {
        "@sonantica/metadata": "workspace:*",
        "@sonantica/shared": "workspace:*",
        "idb-keyval": "^6.2.2",
        "zustand": "5.0.9"
    },
    "devDependencies": {
        "rimraf": "6.1.2",
        "tsup": "8.5.1",
        "typescript": "5.9.3"
    }
}