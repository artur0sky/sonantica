{
    "name": "@sonantica/shared",
    "version": "0.1.0",
    "description": "Shared types, utilities, constants for Sonántica - NO state management",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --external react",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react",
        "clean": "rimraf dist",
        "test": "echo \"No tests yet\"",
        "lint": "echo \"No linting configured yet\""
    },
    "keywords": [
        "sonantica",
        "shared",
        "types",
        "utils",
        "constants"
    ],
    "author": "Sonántica Team",
    "license": "Apache-2.0",
    "peerDependencies": {
        "react": "19.2.3"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/react": "19.2.3",
        "tsup": "8.5.1",
        "typescript": "5.9.3",
        "rimraf": "6.1.2"
    }
}