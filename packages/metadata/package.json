{
    "name": "@sonantica/metadata",
    "version": "0.1.0",
    "description": "Metadata extraction for audio files - Sonántica",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./node": {
            "types": "./dist/node.d.ts",
            "import": "./dist/node.js",
            "require": "./dist/node.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts src/node.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts src/node.ts --format cjs,esm --dts --watch",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "@sonantica/shared": "workspace:*",
        "@sonantica/lyrics": "workspace:*"
    },
    "devDependencies": {
        "@types/node": "22.0.0",
        "tsup": "8.5.1",
        "typescript": "5.9.3"
    },
    "keywords": [
        "metadata",
        "id3",
        "audio",
        "tags",
        "music"
    ],
    "author": "Sonántica Team",
    "license": "Apache-2.0"
}