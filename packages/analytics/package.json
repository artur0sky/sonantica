    "license": "Apache-2.0",
    "dependencies": {
        "@sonantica/shared": "workspace:*",
        "zustand": "4.4.7",
        "uuid": "9.0.1"
    },
    "devDependencies": {
        "@types/uuid": "9.0.7",
        "tsup": "8.0.1",
        "typescript": "5.3.3"
    },
    "peerDependencies": {
        "react": "19.2.3"
    }
}