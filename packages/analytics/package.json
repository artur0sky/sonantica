{
    "name": "@sonantica/analytics",
    "version": "0.1.0",
    "description": "Analytics and telemetry package for Sonántica - Technical Transparency",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup && tsc --emitDeclarationOnly",
